create table veggies(
veggie_id varchar(20),
veggie_name varchar(20),
veggie_price money,
veggie_type varchar(20),
primary key (veggie_id)
);


create table lots(
lot_id varchar(20),
lot_name varchar(20),
quantity_of_veggies varchar(20),
veggie_id varchar(20),
primary key (lot_id)
);



