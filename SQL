CREATE PROCEDURE GeneratePackingSheet 
AS
BEGIN
    SET NOCOUNT ON;

	select * from ORDERS as O, TRUCKS as T, Warehouse as W
	where O.ITEM_ID = W.ITEM_ID and O.TRUCK_ID = T.TRUCK_ID 

END
exec GeneratePackingSheet
