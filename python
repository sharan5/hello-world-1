a = int(input("Enter the number1: "))
print a
b=int(input("Enter the number2: "))
print b
c = int(input("Enter the number3: "))
print c


if ( a*a== b*b+c*c):
    print("a*a==b*b+c*c")
if (b*b==a*a+c*c):
    print("b*b==a*a+c*c")
if (c*c==a*a+b*b):
    print("c*c==a*a+c*c")

elif (a*a!=b*b+c*c) and (b*b!=a*a+c*c) and (c*c!=a*a+c*c):
    print("not possible")









